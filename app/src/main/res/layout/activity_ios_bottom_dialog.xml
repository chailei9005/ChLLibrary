<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_ios_bottom_dialog"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.example.chailei.chllibrary.activity.IosBottomDialogActivity">

    <Button
        android:id="@+id/fragment_ios_bottom_dialog_btn1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="无标题，只有操作" />

    <Button
        android:id="@+id/fragment_ios_bottom_dialog_btn2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="无操作，只有标题" />

    <Button
        android:id="@+id/fragment_ios_bottom_dialog_btn3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="标题＋1个操作" />

    <Button
        android:id="@+id/fragment_ios_bottom_dialog_btn4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="标题＋2个操作" />

    <Button
        android:id="@+id/fragment_ios_bottom_dialog_btn5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="无标题＋3个操作" />

    <Button
        android:id="@+id/fragment_ios_bottom_dialog_btn6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="无标题＋4个操作" />

</LinearLayout>
